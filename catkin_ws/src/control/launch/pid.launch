<launch>

	<!--Linear-->
	<node name="controller"
		  pkg="pid"
		  type="controller"
		  ns="linear_pid"
		  output="screen">
	    <param name="node_name" value="pid" />

	    <!--Default PID Gains (Should load from file eventually)-->
	    <param name="Kp" value="10" />
	    <param name="Ki" value="0.0" />
	    <param name="Kd" value="0.0" />

	    <!--Effort Limits-->
	    <param name="upper_limit" value="1000" />
	    <param name="lower_limit" value="-1000" />

	    <!--Integral Factor Limit-->
	    <param name="windup_limit" value="10" />

	    <!--Set Topics-->
	  	<!--Setpoint will be "/<ns>/setpoint"-->
	  	<!--Enable will be "/<ns>/pid_enable"-->
	  	<param name="topic_from_controller"
	  		   value="/control/TODO" />
	  	<param name="topic_from_plant"
	  		   value="/state_estimation/TODO" />
	</node>

	<!--Angular-->
	<node name="controller"
		  pkg="pid"
		  type="controller"
		  ns="angular_pid"
		  output="screen">
	    <param name="node_name" value="pid" />

	    <!--Default PID Gains (Should load from file eventually)-->
	    <param name="Kp" value="10" />
	    <param name="Ki" value="0.0" />
	    <param name="Kd" value="0.0" />

	    <!--Effort Limits-->
	    <param name="upper_limit" value="1000" />
	    <param name="lower_limit" value="-1000" />

	    <!--Integral Factor Limit-->
	    <param name="windup_limit" value="10" />

	    <!--Set Topics-->
	  	<!--Setpoint will be "/<ns>/setpoint"-->
	  	<!--Enable will be "/<ns>/pid_enable"-->
	  	<param name="topic_from_controller"
	  		   value="/controls/superimposer/TODO" />
	  	<param name="topic_from_plant"
	  		   value="/state_estimation/TODO" />
	</node>


</launch>