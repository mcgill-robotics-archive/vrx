<launch>
  <node name="rate_controller"
   pkg="control"
   type="controller"
   output="screen"
   respawn="false">

    <!--Yaw rate controller params-->
    <param name="yaw_rate/Kp" value="1" type="double"/>
    <param name="yaw_rate/Ki" value="0" type="double"/>
    <param name="yaw_rate/Kd" value="0" type="double"/>

    <param name="yaw_rate/max_windup" value="100" type="double"/>
    <param name="yaw_rate/min_effort" value="-0.1" type="double"/>
    <param name="yaw_rate/max_effort" value="0.1" type="double"/>

    <!--Lateral velocity controller params-->
    <param name="lateral_vel/Kp" value="1" type="double"/>
    <param name="lateral_vel/Ki" value="0" type="double"/>
    <param name="lateral_vel/Kd" value="0" type="double"/>

    <param name="lateral_vel/max_windup" value="100" type="double"/>
    <param name="lateral_vel/min_effort" value="-0.1" type="double"/>
    <param name="lateral_vel/max_effort" value="0.1" type="double"/>

    <!--Forward velocity controller params-->
    <param name="forward_vel/Kp" value="1" type="double"/>
    <param name="forward_vel/Ki" value="0" type="double"/>
    <param name="forward_vel/Kd" value="0" type="double"/>

    <param name="forward_vel/max_windup" value="1" type="double"/>
    <param name="forward_vel/min_effort" value="-0.1" type="double"/>
    <param name="forward_vel/max_effort" value="0.1" type="double"/>

    <!-- Thruster topics -->
    <param name="front_left_thrust_topic" value="/wamv/thrusters/left_front_thrust_cmd" type="string"/>
    <param name="front_right_thrust_topic" value="/wamv/thrusters/right_front_thrust_cmd" type="string"/>
    <param name="back_left_thrust_topic" value="/wamv/thrusters/left_rear_thrust_cmd" type="string"/>
    <param name="back_right_thrust_topic" value="/wamv/thrusters/right_rear_thrust_cmd" type="string"/>

    <!-- Input topics -->
    <param name="odom_topic" value="/wamv/robot_localization/odometry/filtered" type="string"/>
    <param name="setpoint_topic" value="/control/cmd_vel" type="string"/>
  </node>
</launch>
